{
  "expo": {
    "name": "MoveMate",
    "slug": "MoveMate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.MoveMate"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACTIVITY_RECOGNITION"
      ],
      "package": "com.anonymous.MoveMate"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "./android-manifest.plugin.js"
    ],
    "extra": {
      "eas": {
        "projectId": "9e7ebab8-523d-47d2-8a57-372a9ab3754f"
      }
    }
  }
}
